package com.kas.logging.impl;

import java.io.PrintStream;
import com.kas.infra.base.TimeStamp;
import com.kas.infra.utils.RunTimeUtils;

public abstract class StdoutAppender extends ConsoleAppender
{
  //------------------------------------------------------------------------------------------------------------------
  //
  //------------------------------------------------------------------------------------------------------------------
  private ConsoleAppenderConfiguration mConfig = null;
  private PrintStream                  mPrintStream = null;
  
  //------------------------------------------------------------------------------------------------------------------
  //
  //------------------------------------------------------------------------------------------------------------------
  protected StdoutAppender(ConsoleAppenderConfiguration cac)
  {
    super(cac, System.out);
  }
}
